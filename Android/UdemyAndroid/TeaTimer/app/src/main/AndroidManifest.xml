<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="serio.tim.android.com.teatimer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Home.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Timers.BlackTeaTimer" />
        <activity android:name=".Timers.GreenTeaTimer" />
        <activity android:name=".Timers.OolongTeaTimer" />
        <activity android:name=".Timers.WhiteTeaTimer" />
        <activity android:name=".Timers.RooibosTeaTimer" />
        <activity android:name=".Timers.GyokuroTimer" />
        <activity android:name=".Timers.YerbaMateTimer" />
        <activity android:name=".Timers.PuErhTimer"></activity>
    </application>

</manifest>